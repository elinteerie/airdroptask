{
    "builds": [
        {
            "src": "back/wsgi.py",
            "use": "@vercel/python"
        }, 
        {
            "installCommand": "python manage.py migrate"
          }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "back/wsgi.py"
        }
    ]

}